// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Payment {
  id      Int    @id @default(autoincrement())
  client  String
  request_date   DateTime
  paid_date    DateTime
  amount  String
  trafik  String
  number  String
  status  Status
}

enum Status {
  Success
  Error
}
